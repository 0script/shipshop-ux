{"ast":null,"code":"import axios from 'axios';\nimport uniqueId from \"lodash.uniqueid\";\nimport CartItem from '@/components/CartItem.vue';\nexport default {\n  name: 'CartView',\n  components: {\n    CartItem\n  },\n  data() {\n    return {\n      cart: {\n        items: []\n      }\n    };\n  },\n  mounted() {\n    this.cart = this.$store.state.cart;\n    console.log('cart item');\n    console.log(this.cart.items);\n    this.items = this.cart.items;\n    console.log(this.items);\n  },\n  computed: {\n    cartTotalLength() {\n      return this.cart.items.reduce((acc, curlVal) => {\n        return acc += curlVal.quantity;\n      }, 0);\n    },\n    cartTotalPrice() {\n      return this.cart.items.reduce((acc, curlVal) => {\n        return acc += curlVal.product.price * curlVal.quantity;\n      }, 0);\n    }\n  },\n  methods: {\n    removeFromCart(item) {\n      this.cart.items = this.cart.items.filter(i => i.product.id !== item.product.id);\n    }\n  }\n};","map":{"version":3,"names":["axios","uniqueId","CartItem","name","components","data","cart","items","mounted","$store","state","console","log","computed","cartTotalLength","reduce","acc","curlVal","quantity","cartTotalPrice","product","price","methods","removeFromCart","item","filter","i","id"],"sources":["/home/osboxes/0dir/shipshop/shipshop-ux/src/views/CartView.vue"],"sourcesContent":["<template>\n\n    <div class=\"page-cart\">\n        <div class=\"columns is-multiline\">\n            <div class=\"column is-12\">\n                <h1 class=\"title\">Cart</h1>\n            </div>\n\n            <div class=\"column is-12 box\">\n                \n                <table class=\"table is-fullwidth\" v-if=\"cartTotalLength\">\n                    <thead>\n                        <tr>\n                            <th>Product</th>\n                            <th>Price</th>\n                            <th>Quantity</th>\n                            <th>Total</th>\n                            <th></th>\n                        </tr>\n                    </thead>\n                    <tbody>\n\n                        <cart-item\n                            v-for=\"item in cart.items\"\n                            v-bind:key=\"item.product.id\"\n                            :initialItem=\"item\"\n                            v-on:removeFromCart=\"removeFromCart\"\n                            >\n                        </cart-item>\n                    </tbody>\n                </table>\n                <p v-else>You don't have any item in you cart !!!</p>\n            </div>\n\n            <div class=\"column is-12 box\">\n\n                <h2 class=\"subtitle\">Summary</h2>\n                <strong>Rwf {{ cartTotalPrice }}</strong> ,{{ cartTotalLength }} items\n                <hr />\n                <router-link to=\"/cart/checkout\" class=\"button is-dartk\">Checkout</router-link>\n                </div>\n            </div>\n    </div>\n</template>\n\n<script>\n\n    import axios from 'axios'\n    import uniqueId from \"lodash.uniqueid\"\n    import CartItem from  '@/components/CartItem.vue'\n\n    export default {\n        name: 'CartView',\n        components:{\n            CartItem\n        },\n        data(){\n            return{\n                cart:{\n                    items:[]\n                }\n            }\n        },\n        mounted(){\n            this.cart=this.$store.state.cart\n            console.log('cart item')\n            console.log(this.cart.items)\n            this.items=this.cart.items\n            console.log(this.items)\n        },\n        computed:{\n            cartTotalLength(){\n                return this.cart.items.reduce((acc,curlVal)=>{\n                    return acc+=curlVal.quantity\n                },0 )\n            },\n            cartTotalPrice(){\n                return this.cart.items.reduce((acc,curlVal)=>{\n                    return acc+=curlVal.product.price*curlVal.quantity\n                },0 )\n            }\n        },\n        methods:{\n            removeFromCart(item){\n                this.cart.items=this.cart.items.filter(i=>i.product.id !== item.product.id)\n            }\n        }\n    }\n</script>"],"mappings":"AA+CI,OAAOA,KAAI,MAAO,OAAM;AACxB,OAAOC,QAAO,MAAO,iBAAgB;AACrC,OAAOC,QAAO,MAAQ,2BAA0B;AAEhD,eAAe;EACXC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAC;IACPF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,IAAI,EAAC;QACDC,KAAK,EAAC;MACV;IACJ;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAE;IACL,IAAI,CAACF,IAAI,GAAC,IAAI,CAACG,MAAM,CAACC,KAAK,CAACJ,IAAG;IAC/BK,OAAO,CAACC,GAAG,CAAC,WAAW;IACvBD,OAAO,CAACC,GAAG,CAAC,IAAI,CAACN,IAAI,CAACC,KAAK;IAC3B,IAAI,CAACA,KAAK,GAAC,IAAI,CAACD,IAAI,CAACC,KAAI;IACzBI,OAAO,CAACC,GAAG,CAAC,IAAI,CAACL,KAAK;EAC1B,CAAC;EACDM,QAAQ,EAAC;IACLC,eAAeA,CAAA,EAAE;MACb,OAAO,IAAI,CAACR,IAAI,CAACC,KAAK,CAACQ,MAAM,CAAC,CAACC,GAAG,EAACC,OAAO,KAAG;QACzC,OAAOD,GAAG,IAAEC,OAAO,CAACC,QAAO;MAC/B,CAAC,EAAC,EAAE;IACR,CAAC;IACDC,cAAcA,CAAA,EAAE;MACZ,OAAO,IAAI,CAACb,IAAI,CAACC,KAAK,CAACQ,MAAM,CAAC,CAACC,GAAG,EAACC,OAAO,KAAG;QACzC,OAAOD,GAAG,IAAEC,OAAO,CAACG,OAAO,CAACC,KAAK,GAACJ,OAAO,CAACC,QAAO;MACrD,CAAC,EAAC,EAAE;IACR;EACJ,CAAC;EACDI,OAAO,EAAC;IACJC,cAAcA,CAACC,IAAI,EAAC;MAChB,IAAI,CAAClB,IAAI,CAACC,KAAK,GAAC,IAAI,CAACD,IAAI,CAACC,KAAK,CAACkB,MAAM,CAACC,CAAC,IAAEA,CAAC,CAACN,OAAO,CAACO,EAAC,KAAMH,IAAI,CAACJ,OAAO,CAACO,EAAE;IAC9E;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}