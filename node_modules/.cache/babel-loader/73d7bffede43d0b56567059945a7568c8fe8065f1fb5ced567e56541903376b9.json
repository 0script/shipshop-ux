div>\n\n    </div>\n\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport ProductsList from '@/components/ProductsList.vue'\nimport axios from 'axios'\n\nexport default {\n  name: 'HomeView',\n  components: {\n    ProductsList\n  },\n  data(){\n    return{\n      latestProducts:[]\n    }\n  },\n  mounted(){\n    this.getLatestProducts()\n  },\n  methods:{\n    getLatestProducts(){\n      axios\n        .get('/api/v1/latest-products')\n        .then(response=>{\n          this.latestProducts=response.data\n        })\n        .catch(error=>{\n          console.log('ERROR DURING REQUEST')\n          console.log(error)\n        })\n    }\n  }\n}\n</script>\n"],"mappings":"AA0CA;AACA,OAAOA,YAAW,MAAO,+BAA8B;AACvD,OAAOC,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,cAAc,EAAC;IACjB;EACF,CAAC;EACDC,OAAOA,CAAA,EAAE;IACP,IAAI,CAACC,iBAAiB,EAAC;EACzB,CAAC;EACDC,OAAO,EAAC;IACND,iBAAiBA,CAAA,EAAE;MACjBN,KAAI,CACDQ,GAAG,CAAC,yBAAyB,EAC7BC,IAAI,CAACC,QAAQ,IAAE;QACd,IAAI,CAACN,cAAc,GAACM,QAAQ,CAACP,IAAG;MAClC,CAAC,EACAQ,KAAK,CAACC,KAAK,IAAE;QACZC,OAAO,CAACC,GAAG,CAAC,sBAAsB;QAClCD,OAAO,CAACC,GAAG,CAACF,KAAK;MACnB,CAAC;IACL;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}